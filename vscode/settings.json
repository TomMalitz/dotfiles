            "packageVersionsUrl": "https://pkgs.dev.azure.com/NMHC/_packaging/nmdigitalplatform@Local/nuget/v3/index.json",
            // The package url in the host server
            // NuGet  Host  "https://www.nuget.org/packages/{{packageName}}"
