        //     ],
        //     "commands": [
        //         "editor.action.marker.prevInFiles"
        //     ],
        //     "silent": true
        // }
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "[yaml]": {},
    "redhat.telemetry.enabled": false,
    "azureFunctions.showCoreToolsWarning": false,
    "codesnap.showWindowControls": false,
    "codesnap.showWindowTitle": true,
    "codesnap.shutterAction": "copy",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 10px 20px",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "cmake.configureOnOpen": false,
    "haxelib.executable": "C:\\HaxeToolkit\\haxe\\haxe.exe",
    "debug.onTaskErrors": "abort",
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "cmake.showOptionsMovedNotification": false,
    "C_Cpp.dimInactiveRegions": false,
    "codesnap.backgroundColor": "#18191a",
    "terminal.integrated.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.tabSize": 2
    },
    "[postcss]": {
        "editor.tabSize": 2
    },
    "[tf]": {
        "editor.tabSize": 2
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.tabSize": 2
    },
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar",
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.accessibilitySupport": "off",
    "accessibility.signals.lineHasError": {
        "sound": "off"
    },
    "workbench.iconTheme": "minimal-icons-without-explorer-arrows",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.gpuAcceleration": "on",
    "nugetpackagemanagergui.packageSources": [
        {
            "sourceName": "nmdigitalplatform@Local",
            "preRelease": true,
            "authorization": {
                // The auth types (basicAuth / none)
                "authType": "basicAuth",
                // It is required if authType is equal to basicAuth
                "username": "ali.asadi",
                // It is required if authType is equal to basicAuth
                "password": "ACCESS_TOKEN_KEY"
