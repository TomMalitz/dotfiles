{
    "settingsSync.keybindingsPerPlatform": false,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "window.zoomLevel": 2,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}",
    // poimandres custom colors
    "workbench.colorCustomizations": {
        "editorCursor.background": "#050505",
        "editorCursor.foreground": "#5de4c7",
        "scrollbarSlider.background": "#28313a",
        "selection.background": "#323849",
        "statusBar.background": "#0f1015",
        "quickInput.background": "#0f1015",
        "statusBarItem.errorBackground": "#371c2a",
        "titleBar.activeBackground": "#0f1015",
        "terminal.background": "#0f1015",
        "panel.background": "#0f1015",
        "sideBar.background": "#0f1015",
        "statusBarItem.warningBackground": "#371c2a",
        "debugView.exceptionLabelBackground": "#371c2a",
        "debugExceptionWidget.background": "#371c2a"
        // "quickInput.background": "#1d252c",
        // "quickInputList.focusBackground": "#28313a",
    },
    // monochrome custom colors
    // "workbench.colorCustomizations": {
    //     "selection.background": "#424241"
    // },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/AppData/Local/Temp": true
    },
    "breadcrumbs.enabled": false,
    "debug.console.fontSize": 12,
    "zenMode.showTabs": "none",
    "zenMode.fullScreen": false,
    "zenMode.hideStatusBar": false,
    "workbench.panel.defaultLocation": "bottom",
    "workbench.panel.opensMaximized": "never",
    "workbench.editor.showTabs": "none",
    "workbench.quickOpen.preserveInput": true,
    "workbench.quickOpen.closeOnFocusLost": true,
    "workbench.statusBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.activityBar.location": "hidden",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.tree.renderIndentGuides": "none",
    "workbench.reduceMotion": "off",
    "workbench.editor.showIcons": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.expand": false,
    "explorer.decorations.colors": false,
    // "background.interval": 1000,
    "vscode_custom_css.imports": [
        "file:///C:/Users/tmali/Documents/vscode-custom/vscode-custom.css",
        "file:///C:/Users/NM320042/Documents/vscode-custom/vscode-custom.css"
    ],
    "background.enabled": true,
    "background.fullscreen": {
        "images": [
            "https://images.unsplash.com/photo-1521489871110-81dc5a61dbda?q=80&w=3737&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D",
            // "file:///C:/Users/NM320042/Documents/vscode-custom/mountain-range.jpg",
            // "file:///C:/Users/tmali/Documents/vscode-custom/mountain-range.jpg",
        ],
        "opacity": 0.97,
        "position": "50%",
        "size": "cover"
    },
    "emmet.includeLanguages": {
        "razor": "html"
    },
    "prettier.semi": false,
    "prettier.printWidth": 110,
    "scm.diffDecorations": "none",
    "gitlens.currentLine.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.snippetSuggestions": "top",
    "editor.lightbulb.enabled": "off",
    "editor.renderLineHighlight": "line",
    "editor.renderWhitespace": "none",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "phase",
    "editor.cursorSurroundingLines": 5,
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.glyphMargin": true,
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.indentation": false,
    "editor.detectIndentation": true,
    // "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.codeActionsOnSave": {},
    "editor.smoothScrolling": true,
    "editor.lineNumbers": "on",
    "editor.occurrencesHighlight": "off",
    "editor.fontSize": 12,
    "editor.lineHeight": 1.1,
    "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
    "editor.codeLens": false,
    "editor.rulers": [],
    "editor.minimap.enabled": false,
    "editor.colorDecorators": false,
    "tailwindCSS.colorDecorators": false,
    "terminals.multiplexer": "screen",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "terminal.integrated.defaultProfile.windows": "powershell7",
    "terminal.integrated.profiles.windows": {
        "powershell7": {
            "path": "c:/Program Files/PowerShell/7/pwsh.exe",
        },
        "lazygit": {
            "path": "c:/Program Files/PowerShell/7/pwsh.exe",
            "args": [
                "-Command",
                "lazygit"
            ]
        }
    },
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.useWslProfiles": false,
    "volar.codeLens.references": false,
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": ";",
    "vim.handleKeys": {
        "<C-f>": true,
        "<C-j>": false,
        "<C-v>": false,
        "<C-h>": false
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Esc>",
                "<Esc>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "#"
            ],
            "after": [
                "_"
            ]
        },
        {
            "before": [
                "<leader>",
                "m",
                "p"
            ],
            "commands": [
                "markdown.showPreview"
            ]
        },
        {
            "before": [
                "<leader>",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditorRight"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "editor.inlayHints.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "z"
            ],
            "commands": [
                "workbench.action.toggleZenMode"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.format"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "g"
            ],
            "commands": [
                "git.viewFileHistory"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "l"
            ],
            "commands": [
                "gitlens.toggleLineBlame"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "workbench.action.openRecent"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "commands": [
                "workbench.profiles.actions.switchProfile"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen",
            ]
        },
        {
            "before": [
                "space"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "workbench.action.quickOpenPreviousRecentlyUsedEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "g"
            ],
            "commands": [
                "workbench.action.quickTextSearch"
                // "find-it-faster.findWithinFiles"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        // {
        //     "before": [
        //         "]",
        //         "d"
        //     ],
        //     "commands": [
        //         "editor.action.marker.nextInFiles"
        //     ],
