            // Azure  Host "https://pkgs.dev.azure.com/username/guid/_packaging/guid/nuget/v3/query2"
            // ProGet Host:"http://localhost/nuget/Test-Feed/v3/search"
            "searchUrl": "https://azuresearch-usnc.nuget.org/query",
